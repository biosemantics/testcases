  <taxon_identification />
  <description>
    <statement id="2.txtp0.txt-0">
      <text>nectar glands ( 1 - ) 2 - 5 ( - 10 + ) , lateral , two on each side of lateral stamen ,
        median glands absent .</text>
      <structure constraint="nectar" id="o1" name="gland">
        <character char_type="range_value" from="1" name="atypical_count" to="2" to_inclusive="false" />
        <character char_type="range_value" from="5" from_inclusive="false" name="atypical_count" to="10" upper_restricted="false" />
        <character char_type="range_value" from="2" name="count" to="5" />
        <character name="position" value="lateral" />
        <character constraint="on side" constraintid="o1" name="count" value="2" />
      </structure>
      <structure id="o2" name="side" />
      <structure constraint="lateral" id="o3" name="stamen" />
      <structure constraint="median" id="o4" name="gland">
        <character name="count" value="absent" />
      </structure>
      <relation from="o2" id="r1" name="part_of" negation="false" to="o3" />
    </statement>
  </description>
</treatment>
