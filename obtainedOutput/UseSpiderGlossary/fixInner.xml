  <description type="morphology">
    <statement id="d0_s0">
      <text>Proximal leaves larger than distal leaves.</text>
      <biological_entity constraint="proximal" id="o138" name="leaf" name_original="leaves" type="structure">
        <character constraint="than distal leaves" constraintid="o139" is_modifier="false" name="size" value="larger" />
      </biological_entity>
      <biological_entity constraint="distal" id="o139" name="leaf" name_original="leaves" type="structure" />
    </statement>
    <statement id="d0_s1">
      <text>Basal and proximal cauline 1-nerved, narrowly oblanceolate, (50–) 90–150× (2–) 4–9 (–12) mm wide;</text>
      <biological_entity constraint="basal and proximal cauline" id="o140" name="leaf" name_original="leaves" type="structure">
        <character is_modifier="false" name="architecture" value="1-nerved" />
        <character is_modifier="false" modifier="narrowly" name="shape" value="oblanceolate" />
        <character char_type="range_value" from="50" from_unit="mm" name="width" to="90" to_inclusive="false" to_unit="mm" />
        <character char_type="range_value" from="90" from_unit="mm" name="width" to="150" to_unit="mm" />
        <character char_type="range_value" from="2" from_unit="mm" name="width" to="4" to_inclusive="false" to_unit="mm" />
        <character char_type="range_value" from="9" from_inclusive="false" from_unit="mm" name="width" to="12" to_unit="mm" />
        <character char_type="range_value" from="4" from_unit="mm" name="width" to="9" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s2">
      <text>Basal and proximal cauline leaves red;</text>
      <biological_entity constraint="basal and proximal cauline" id="o141" name="leaf" name_original="leaves" type="structure">
        <character is_modifier="false" name="coloration" value="red" />
      </biological_entity>
    </statement>
    <statement id="d0_s3">
      <text>Outer scales 20-25, 0.5-1 mm, contrasting with 35-40+, 4.5-6+ mm inner bristles.</text>
      <biological_entity constraint="outer" id="o142" name="scale" name_original="scales" type="structure">
        <character char_type="range_value" from="20" name="quantity" to="25" />
        <character char_type="range_value" from="0.5" from_unit="mm" name="some_measurement" to="1" to_unit="mm" />
      </biological_entity>
      <biological_entity constraint="inner" id="o143" name="bristle" name_original="bristles" type="structure">
        <character char_type="range_value" from="35" is_modifier="true" name="quantity" to="40" upper_restricted="false" />
        <character char_type="range_value" from="4.5" from_unit="mm" is_modifier="true" name="some_measurement" to="6" to_unit="mm" upper_restricted="false" />
      </biological_entity>
      <relation from="o143" id="r14" name="contrasting with" negation="false" to="o143" />
    </statement>
    <statement id="d0_s4">
      <text>Leaves opposite, or proximal opposite and distal alternate.</text>
      <biological_entity id="o144" name="leaf" name_original="leaves" type="structure">
        <character is_modifier="false" name="arrangement" value="opposite" />
      </biological_entity>
      <biological_entity constraint="proximal" id="o145" name="leaf" name_original="leaves" type="structure">
        <character is_modifier="false" name="arrangement" value="opposite" />
      </biological_entity>
      <biological_entity constraint="distal" id="o146" name="leaf" name_original="leaves" type="structure">
        <character is_modifier="false" name="arrangement" value="alternate" />
      </biological_entity>
    </statement>
    <statement id="d0_s5">
      <text>Pappi of 2–3 awns plus shorter bristles or scales, or wholly of minute scales.</text>
      <biological_entity id="o147" name="pappus" name_original="pappi" type="structure" />
      <biological_entity id="o148" name="awn" name_original="awns" type="structure">
        <character char_type="range_value" from="2" is_modifier="true" name="quantity" to="3" />
      </biological_entity>
      <biological_entity constraint="shorter" id="o149" name="bristle" name_original="bristles" type="structure" />
      <biological_entity constraint="shorter" id="o150" name="scale" name_original="scales" type="structure" />
      <biological_entity id="o151" name="scale" name_original="scales" type="structure">
        <character is_modifier="true" name="size" value="minute" />
      </biological_entity>
      <relation from="o147" id="r15" name="consist_of" negation="false" to="o148" />
      <relation from="o147" id="r16" modifier="wholly" name="consists_of" negation="false" to="o151" />
    </statement>
    <statement id="d0_s6">
      <text>Tips of outer and mid phyllaries acute;</text>
      <biological_entity id="o152" name="tip" name_original="tips" type="structure">
        <character is_modifier="false" name="shape" value="acute" />
      </biological_entity>
      <biological_entity constraint="outer and mid" id="o153" name="phyllary" name_original="phyllaries" type="structure" />
      <relation from="o152" id="r17" name="part_of" negation="false" to="o153" />
    </statement>
    <statement id="d0_s7">
      <text>Mid-stems (30-100+);</text>
      <biological_entity id="o154" name="mid-stem" name_original="mid-stems" type="structure">
        <character char_type="range_value" from="30" name="atypical_quantity" to="100" upper_restricted="false" />
      </biological_entity>
    </statement>
    <statement id="d0_s8">
      <text>Cypselae flattened, 2– (3–4) -nerved;</text>
      <biological_entity id="o155" name="cypsela" name_original="cypselae" type="structure">
        <character is_modifier="false" name="shape" value="flattened" />
        <character is_modifier="false" name="architecture" value="2-(3-4)-nerved" />
      </biological_entity>
    </statement>
    <statement id="d0_s9">
      <text>pappi of outer, shorter bristles or scales plus inner, longer bristles;</text>
      <biological_entity id="o156" name="pappus" name_original="pappi" type="structure" constraint="bristle">
        <character is_modifier="false" name="position" value="inner" />
      </biological_entity>
      <biological_entity constraint="shorter" id="o157" name="bristle" name_original="bristles" type="structure">
        <character is_modifier="true" name="position" value="outer" />
      </biological_entity>
      <biological_entity id="o158" name="scale" name_original="scales" type="structure" />
      <biological_entity constraint="longer" id="o159" name="bristle" name_original="bristles" type="structure" />
      <relation from="o156" id="r18" name="part_of" negation="false" to="o157" />
      <relation from="o156" id="r19" name="part_of" negation="false" to="o158" />
    </statement>
    <statement id="d0_s10">
      <text>apices of inner erect, abaxial faces gray-tomentose, ± twisted.</text>
      <biological_entity id="o160" name="apex" name_original="apices" type="structure" constraint="pappus">
        <character is_modifier="false" name="orientation" value="erect" />
      </biological_entity>
      <biological_entity constraint="inner" id="o161" name="pappus" name_original="pappi" type="structure" />
      <biological_entity constraint="abaxial" id="o162" name="face" name_original="faces" type="structure">
        <character is_modifier="false" name="pubescence" value="gray-tomentose" />
        <character is_modifier="false" modifier="more or less" name="architecture" value="twisted" />
      </biological_entity>
      <relation from="o160" id="r20" name="part_of" negation="false" to="o161" />
    </statement>
    <statement id="d0_s11">
      <text>Phyllaries in 5–7 series, weakly unequal, dark green to brownish, lanceolate (outer) to linear (inner), abaxial faces without (or with very obscure) glutinous ridge;</text>
      <biological_entity id="o163" name="phyllary" name_original="phyllaries" type="structure">
        <character is_modifier="false" modifier="weakly" name="size" notes="" value="unequal" />
        <character is_modifier="false" name="coloration" notes="[duplicate value]" value="dark green" />
        <character is_modifier="false" name="coloration" notes="[duplicate value]" value="brownish" />
        <character char_type="range_value" from="dark green" name="coloration" to="brownish" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" value="lanceolate" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" value="linear" />
        <character char_type="range_value" from="lanceolate" name="shape" to="linear" />
      </biological_entity>
      <biological_entity id="o164" name="series" name_original="series" type="structure">
        <character char_type="range_value" from="5" is_modifier="true" name="quantity" to="7" />
      </biological_entity>
      <biological_entity id="o166" name="ridge" name_original="ridge" type="structure">
        <character is_modifier="true" name="coating" value="glutinous" />
      </biological_entity>
      <relation from="o163" id="r21" name="in" negation="false" to="o164" />
      <relation from="o165" id="r22" name="without" negation="false" to="o166" />
    </statement>
    <statement id="d0_s12">
      <text>outer and middle appressed or apices spreading, at least outer irregularly spiny-fringed, finely serrulate, spines slender, 3–5 mm;</text>
      <biological_entity constraint="abaxial" id="o165" name="face" name_original="faces" type="structure" />
      <biological_entity constraint="outer" id="o167" name="face" name_original="faces" type="structure" />
      <biological_entity constraint="middle" id="o168" name="face" name_original="faces" type="structure">
        <character is_modifier="false" name="fixation_or_orientation" value="appressed" />
      </biological_entity>
      <biological_entity constraint="outer" id="o169" name="apex" name_original="apices" type="structure">
        <character is_modifier="false" name="orientation" value="spreading" />
        <character is_modifier="false" modifier="at-least" name="position" value="outer" />
        <character is_modifier="false" modifier="irregularly" name="architecture" value="spiny-fringed" />
        <character is_modifier="false" modifier="finely" name="architecture_or_shape" value="serrulate" />
      </biological_entity>
      <biological_entity id="o170" name="spine" name_original="spines" type="structure">
        <character is_modifier="false" name="size" value="slender" />
        <character char_type="range_value" from="3" from_unit="mm" name="some_measurement" to="5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="d0_s13">
      <text>apices of inner erect, abaxial faces gray-tomentose, ± twisted.</text>
      <biological_entity id="o171" name="apex" name_original="apices" type="structure" constraint="face">
        <character is_modifier="false" name="pubescence" value="gray-tomentose" />
        <character is_modifier="false" modifier="more or less" name="architecture" value="twisted" />
      </biological_entity>
      <biological_entity constraint="abaxial" id="o172" name="face" name_original="faces" type="structure">
        <character is_modifier="true" name="position" value="inner" />
        <character is_modifier="true" name="orientation" value="erect" />
      </biological_entity>
      <relation from="o171" id="r23" name="part_of" negation="false" to="o172" />
    </statement>
    <statement id="d0_s14">
      <text>Pistillate heads: phyllaries 12–30 (–80+) in 1–8+ series, outer, (1–) 5–8 distinct or ± connate, the rest ± connate.</text>
      <biological_entity id="o173" name="head" name_original="heads" type="structure">
        <character is_modifier="true" name="architecture" value="pistillate" />
      </biological_entity>
      <biological_entity id="o174" name="phyllary" name_original="phyllaries" type="structure">
        <character char_type="range_value" from="30" from_inclusive="false" name="atypical_quantity" to="80" upper_restricted="false" />
        <character char_type="range_value" constraint="in series" constraintid="o175" from="12" name="quantity" to="30" />
      </biological_entity>
      <biological_entity id="o175" name="series" name_original="series" type="structure">
        <character char_type="range_value" from="1" is_modifier="true" name="quantity" to="8" upper_restricted="false" />
      </biological_entity>
      <biological_entity constraint="outer" id="o176" name="phyllary" name_original="phyllaries" type="structure">
        <character char_type="range_value" from="1" name="atypical_quantity" to="5" to_inclusive="false" />
        <character char_type="range_value" from="5" name="quantity" to="8" />
        <character is_modifier="false" name="fusion" value="distinct" />
        <character is_modifier="false" modifier="more or less" name="fusion" value="connate" />
      </biological_entity>
      <biological_entity id="o177" name="rest" name_original="rest" type="structure">
        <character is_modifier="false" modifier="more or less" name="fusion" value="connate" />
      </biological_entity>
    </statement>
    <statement id="d0_s15">
      <text>Pappi usually of 2 series, outer of readily falling, short bristles, inner persistent or falling as unit, of basally connate, usually longer, plumose bristles.</text>
      <biological_entity id="o178" name="pappus" name_original="pappi" type="structure" />
      <biological_entity id="o179" name="series" name_original="series" type="structure">
        <character is_modifier="true" name="quantity" value="2" />
      </biological_entity>
      <biological_entity constraint="outer" id="o180" name="pappus" name_original="pappi" type="structure" />
      <biological_entity id="o181" name="bristle" name_original="bristles" type="structure">
        <character is_modifier="true" modifier="readily" name="life_cycle" value="falling" />
        <character is_modifier="true" name="height_or_length_or_size" value="short" />
      </biological_entity>
      <biological_entity constraint="inner" id="o182" name="pappus" name_original="pappi" type="structure">
        <character is_modifier="false" name="duration" value="persistent" />
        <character constraint="as unit" constraintid="o183" is_modifier="false" name="life_cycle" value="falling" />
      </biological_entity>
      <biological_entity id="o183" name="unit" name_original="unit" type="structure" />
      <biological_entity id="o184" name="bristle" name_original="bristles" type="structure">
        <character is_modifier="true" modifier="basally" name="fusion" value="connate" />
        <character is_modifier="true" modifier="usually" name="length_or_size" value="longer" />
        <character is_modifier="true" name="shape" value="plumose" />
      </biological_entity>
      <relation from="o178" id="r24" name="consist_of" negation="false" to="o179" />
      <relation from="o180" id="r25" name="part_of" negation="false" to="o181" />
      <relation from="o182" id="r26" name="part_of" negation="false" to="o184" />
    </statement>
    <statement id="d0_s16">
      <text>Outer phyllaries appressed.</text>
      <biological_entity constraint="outer" id="o185" name="phyllary" name_original="phyllaries" type="structure">
        <character is_modifier="false" name="fixation_or_orientation" value="appressed" />
      </biological_entity>
    </statement>
    <statement id="d0_s17">
      <text>Middle phyllaries appressed.</text>
      <biological_entity constraint="middle" id="o186" name="phyllary" name_original="phyllaries" type="structure">
        <character is_modifier="false" name="fixation_or_orientation" value="appressed" />
      </biological_entity>
    </statement>
    <statement id="d0_s18">
      <text>Inner phyllaries lanceolate.</text>
      <biological_entity constraint="inner" id="o187" name="phyllary" name_original="phyllaries" type="structure">
        <character is_modifier="false" name="shape" value="lanceolate" />
      </biological_entity>
    </statement>
    <statement id="d0_s19">
      <text>Outer pappi 20-30;</text>
      <biological_entity constraint="outer" id="o188" name="pappus" name_original="pappi" type="structure">
        <character char_type="range_value" from="20" name="quantity" to="30" />
      </biological_entity>
    </statement>
    <statement id="d0_s20">
      <text>Inner pappi 10-50;</text>
      <biological_entity constraint="inner" id="o189" name="pappus" name_original="pappi" type="structure">
        <character char_type="range_value" from="10" name="quantity" to="50" />
      </biological_entity>
    </statement>
  </description>
