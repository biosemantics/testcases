      <biological_entity constraint="abaxial" id="o353" name="face" name_original="faces" type="structure">
        <character is_modifier="false" modifier="usually" name="coloration" value="gland-dotted" />
        <character is_modifier="false" name="pubescence" value="hirsute" />
        <character is_modifier="false" name="pubescence" value="puberulent" />
        <character is_modifier="false" name="pubescence" value="pubescent" />
        <character is_modifier="false" name="pubescence" value="scabrous" />
        <character is_modifier="false" name="pubescence" value="stipitate-glandular" />
        <character is_modifier="false" name="pubescence" value="scabrous" />
        <character is_modifier="false" name="pubescence" value="stipitate-glandular" />
        <character is_modifier="false" modifier="sometimes" name="pubescence" value="glabrate" />
      </biological_entity>
      <biological_entity constraint="adaxial" id="o354" name="face" name_original="faces" type="structure">
        <character is_modifier="false" modifier="mostly" name="pubescence" notes="[duplicate value]" value="puberulent" />
        <character is_modifier="false" name="pubescence" notes="[duplicate value]" value="scabrous-hirsute" />
        <character char_type="range_value" from="mostly puberulent" name="pubescence" to="scabrous-hirsute" />
        <character is_modifier="false" name="pubescence" value="glabrescent" />
        <character is_modifier="false" modifier="sometimes" name="pubescence" value="glabrate" />
        <character is_modifier="false" name="pubescence" value="glabrous" />
      </biological_entity>
    </statement>
    <statement id="d0_s12">
      <text>Pappi of 1–4 scales in 1 series, or of, 5–8 scales in 2, abruptly unequal series, longest scales, 1–, 2–8 mm. 2n = 12;</text>
