<?xml version="1.0" encoding="UTF-8"?>
<treatment>
  <meta>
    <source>CycadType2</source>
    <test_target>compound character states, e.g. bright green</test_target>
  </meta>
  <taxon_identification />
  <description>
    <statement id="5.txtp0.txt-0">
      <text>Leaves light or bright green, semiglossy, 50-100 cm long, flat (not keeled )in section, with 30-80 leaflets;</text>
      <structure id="o1" name_original="leaves" name="leaf">
        <character name="coloration" value="light" />
        <character name="coloration" value="bright green" />
        <character name="reflectance" value="semiglossy" />
        <character char_type="range_value" name="length" from="50" from_unit="cm" to="100" to_unit="cm" />
        <character name="prominence_or_shape" value="flat" modifier="in section" />
        <character name="shape" value="keeled" modifier="not" in_brackets="true" />
      </structure>
      <structure id="o2" name_original="leaflets" name="leaflet">
        <character char_type="range_value" name="count" from="30" to="80" />
      </structure>
      <relation id="r1" name="with" from="o1" to="o2" negation="false" />
    </statement>
  </description>
</treatment>
